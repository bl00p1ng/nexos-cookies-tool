{
  "name": "nexos-cookies-tool",
  "version": "1.0.0",
  "description": "Aplicación de escritorio para Windows y Mac OS que se integra con el navegador de Ads Power de forma que abra perfiles del navegador y navegue por una serie de sitios web con el fin de solictar cookies",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "node --watch src/main.js",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "electron-builder",
    "build-win": "electron-builder --win",
    "build-mac": "electron-builder --mac",
    "dist": "electron-builder --publish=never"
  },
  "keywords": [],
  "author": "Andrés López <blooping@protonmail.com>",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "axios": "^1.12.2",
    "commander": "^14.0.0",
    "csv-parse": "^6.1.0",
    "electron": "^38.1.0",
    "electron-store": "^10.1.0",
    "playwright": "^1.55.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron-builder": "^26.0.12",
    "wait-on": "^8.0.5"
  }
}
